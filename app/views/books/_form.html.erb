<%= error%>

<%= form_with model: book, local: true do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.label :release %><br>
    <%= form.datetime_field :release %>
  </div>

  <div>
    <br>
    <%= form.submit %>
  </div>
<% end %>
